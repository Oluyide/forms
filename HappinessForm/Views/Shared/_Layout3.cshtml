            jQuery(document).ready(function($) {
                $('.counter').counterUp({
                    delay: 100,
                    time: 1200
                });

                $(".knob").knob();

            });

            $(function () {
                $('.listbox').multiselect({
                    includeSelectAllOption: true
                });

                //var newDate = new Date();
                //var formattedString = [newDate.getMonth() + 1, newDate.getDate(), newDate.getFullYear()].join("/");


                $('#start').val();//.val('0'+formattedString);
                $('#end').val();//.val('0'+formattedString);

                $('#value').val('<!DOCTYPE HTML>' + '\n' + document.documentElement.outerHTML);
            });

            $(function () {
                var ipd = 0.00;
                var opd = 0.00;
                var tot = 0.00;

                $("tr.grid-row").each(function () {
                    var tr = $(this);
                    var rows = $(tr).children();
                    var ipdamount = rows[3].innerHTML;
                    var opdamount = rows[2].innerHTML;
                    var totamount = rows[4].innerHTML;
                    ipdamount = ipdamount.replace(",", ".");
                    ipdamount = parseFloat(ipdamount);

                    opdamount = opdamount.replace(",", ".");
                    opdamount = parseFloat(opdamount);

                    totamount = totamount.replace(",", ".");
                    totamount = parseFloat(totamount);

                    ipd += ipdamount;
                    opd += opdamount;
                    tot += totamount;
                });

                ipd = ipd.toFixed(2);
                opd = opd.toFixed(2);
                tot = tot.toFixed(2);
                var trTotals = "<tr><td></td><td></td><td style='font-weight:bold;'>" + opd + "</td><td style='font-weight:bold;'>" + ipd + "</td><td style='font-weight:bold;'>" + tot + "</td><td></td></tr>"
                $("tr.grid-row:last").after(trTotals);
                $("#salaryTotal").text("Salary total is - " + ipd);

            });


    </script>

