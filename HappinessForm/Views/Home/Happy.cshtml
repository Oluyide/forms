    <script type="text/javascript" src="asset2/plugins/jquery-validation/js/jquery.validate.min.js"></script>

    <!--wizard initialization-->
    <script src="asset2/pages/jquery.wizard-init.js" type="text/javascript"></script>

</body>
</html>






