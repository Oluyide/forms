                                    </div>

                                    <div class="form-group col-md-3">
                                        @Html.LabelFor(model => model.Others, new { @class = "form-group", @id = "" })
                                        @Html.TextBoxFor(model => model.Others, new { @placeholder = "Name of staff if you know", @class = "form-group", @id = "" })
                                        @Html.DropDownListFor(model => model.OthersRating, Model.OthersRate, new { @class = "form-group", @id = "" })
                                    </div>
                                </fieldset>
                                <div class="form-group col-md-12">
                                    @Html.Label("Please Indicate the name of one member of staff that best attend to you", new { @placeholder = "Your best staff", @class = "labelcol", @id = "" })
                                    @Html.TextBoxFor(model => model.YourbestStaff, new { @placeholder = "Your best staff", @class = "form-group col-md-12", @id = "", required = "" })
                                </div>

                                <div class="form-group col-md-12">
                                    @Html.Label("Comment and Suggestion", new { @placeholder = "Your best staff", @class = "form-group col-md-12", @id = "" })
                                    @Html.TextAreaFor(model => model.Others, new { @placeholder = "Comment and Suggestion", @class = "form-group col-md-12", @id = "", required = "" })
                                </div>

                                <center><input type="submit" value="Submit" class="btn btn-primary" /></center>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
