    <!-- Top content -->
    <div class="top-content">

        <div class="inner-bg">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8 col-sm-offset-2 text">
                        <h1>@*<strong>Lagoon Hospital</strong>*@  Patients Satisfaction Form</h1>
                        <div class="description">
                            @*<p>
                                    Patient Satisfaction Form
                                </p>*@
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 form-box">
                        <div class="form-top">
                            <div class="form-top-left">
                                <h3>Your Happiness Our Concern.....</h3>
                                <p>Fill in the form below to rate our services</p>
                            </div>
                            <div class="form-top-right">
                                <i class="fa fa-envelope"></i>
                            </div>
                        </div>
                        <div class="form-bottom contact-form">

                            @using (Html.BeginForm("Index", "Home", FormMethod.Post))
                            {
                                <fieldset>
                                    <legend>Personal Info</legend>
                                    <div class="form-group col-md-12 col-lg-12 col-sm-12">
                                        @Html.Label("Full Name")
                                        @Html.TextBoxFor(x => x.Name, new { @placeholder = "Full Name", @class = "form-control", @id = "contact-email", required = "" })
                                        @*<input type="text" name="email" placeholder="Email..." class="contact-email form-control" id="contact-email">*@
                                    </div>
                                    <div class="form-group col-md-4 col-sm-4">
                                        @Html.Label("Date")
                                        @Html.TextBoxFor(x => x.DateOfVisit, "{0:dd/MM/yyyy}", new { @placeholder = "Date", @class = "form-control", @id = "dateofVisit", required = "" })
                                        @*<input type="text" name="subject" placeholder="Subject..." class="contact-subject form-control" id="contact-subject">*@
                                    </div>
                                    <div class="form-group col-md-4">
                                        @Html.Label("Time In")
                                        @Html.TextBoxFor(x => x.TimeIn, new { @placeholder = "TimeIn", @class = "form-control", @id = "TimeIn", required = "" })
                                    </div>
                                    <div class="form-group; col-md-4">
                                        @Html.Label("Time Out")
                                        @Html.TextBoxFor(x => x.TimeOut, new { @placeholder = "TimeOut", @class = "form-control", @id = "TimeOut", required = "" })
                                    </div>

                                    <div class="form-group col-md-12">
                                        @Html.Label("Company")
                                        @Html.TextBoxFor(x => x.Company, new { @placeholder = "Company", @class = "form-control", @id = "", required = "" })
                                    </div>

                                    <div class="form-group col-md-12">
                                        @Html.Label("Encounter Number")
                                        @Html.TextBoxFor(x => x.EncounterNo, new { @placeholder = "EncounterNo", @class = "form-control", @id = "", required = "" })
                                    </div>

                                    <div class="form-group; col-md-6">
                                        @Html.LabelFor(model => model.PlanList)
                                        @Html.DropDownListFor(model => model.PlanCategoryId, Model.PlanList, new { @placeholder = "EncounterNo", @class = "form-control", @id = "" })
                                    </div>

                                    <div class="form-group; col-md-6">
                                        @Html.LabelFor(model => model.ClinicList)
                                        @Html.DropDownListFor(model => model.ClinicId, Model.ClinicList, new { @placeholder = "EncounterNo", @class = "form-control", @id = "" })
                                    </div>

                                </fieldset>
                                <fieldset>
