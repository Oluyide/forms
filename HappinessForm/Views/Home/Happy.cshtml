                                        @Html.DropDownListFor(model => model.VitalSignsRating, Model.VitalSignsRate, new { @placeholder = "Name of staff if you know?", @class = "form-group", @id = "" })
                                    </div>
                                    <div class="form-group col-md-3">
                                        @Html.LabelFor(model => model.Consultations, new { @class = "form-group", @id = "" })
